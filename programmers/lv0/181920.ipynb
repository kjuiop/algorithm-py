   "source": [
    "def solution(start_num, end_num):\n",
    "    answer = []\n",
    "    for i in range(start_num, end_num + 1):\n",
    "        answer.append(i)\n",
    "    return answer\n",
    "\n",
    "print(solution(3, 10))"
   ]
  }
